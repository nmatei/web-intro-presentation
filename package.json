{
  "name": "web-intro-presentation",
  "version": "2.0.0",
  "description": "Intro Presentation to Web Development for students that want to learn basics of HTML/CSS/JS",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf docs",
    "clear": "npm run clean && rimraf node_modules",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.prod.js --mode production && npm run copy-resources && git add docs/*",
    "deploy": "npm run build && git commit -m \"[build] new docs\" && git push",
    "copy-resources": "Xcopy /E /C /I \"src/images\" \"docs/images\" && Xcopy /E /C /I \"src/resources\" \"docs/resources\"",
    "start": "webpack serve --open --config webpack.dev.js",
    "serve": "serve docs",
    "qa": "yarn build && yarn serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmatei/web-intro-presentation.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nmatei/web-intro-presentation/issues"
  },
  "homepage": "https://github.com/nmatei/web-intro-presentation#readme",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.4",
    "style-loader": "^3.3.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.12.0"
  }
}
