{
  "name": "web-intro-presentation",
  "version": "2.0.0",
  "description": "Intro Presentation to Web Development for students that want to learn basics of HTML/CSS/JS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "npm run clean && rimraf node_modules",
    "clean": "rimraf docs",
    "build": "npm run clean && webpack --config webpack.prod.js --mode production && npm run copy-resources && git add docs/*",
    "copy-resources": "Xcopy /E /C /I \"src/images\" \"docs/images\"",
    "start": "webpack serve --open --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmatei/web-intro-presentation.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nmatei/web-intro-presentation/issues"
  },
  "homepage": "https://github.com/nmatei/web-intro-presentation#readme",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "style-loader": "^3.3.1",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3"
  }
